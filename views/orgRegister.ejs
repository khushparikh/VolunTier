  <div class="container" id="container">
    <div class="form-container sign-up-container">
        <form action="/orgRegister" method="POST">
            <h3>What Skills Are Required at Your Organization?</h3>
        <div class="social-container">
        
            <% for(let category of categories2) { %>
                <input type="checkbox" name= "<%= category %>" value = "<%= category %>" class = "btn-check" id= "<%= category %>"> </input>
                <label class="btn btn-outline-primary" for= "<%=category%>"><%= category %></label>
       
            <% } %>
           
        </div>
        <div class="mb-3"><button>Submit</button></div> 
    </div>
    <div class="form-container sign-in-container">
        <div class="card-body">
        <div>
            <label for= "username">Enter Username:</label>
            <input type = "text" name = "username" id = "username"  placeholder = "Username" required>
        </div>
        <div>
            <label for= "password">Enter Password:</label>
            <input type = "password" name = "password" id = "password"  placeholder = "Password" required>
        </div>
        <label for= "name">Name:</label>
        <input type = "text" name = "orgName" id = "orgName" placeholder = "Name" required>
        <label for = "zipCode">Zip Code:</label> 
        <input type = "number" name = "zipCode" id = "zipCode" placeholder = "Zip Code" required> 
        <label for = "phoneNum">Phone number</label>
        <input type = "number" name = "phoneNum" id = "phoneNum" placeholder = "Phone number" required>
        <label for = "description">Description of Organization</label>
        <input type = "text" name = "description" id = "description" placeholder = "Description" required>
    </div>
</div>
  
    <div class="overlay-container">
      <div class="overlay">
        <div class="overlay-panel overlay-left">
          <h1>Welcome!</h1>
          <p>Please enter your Organization's Information So We Can Help You Find the Perfect Applicant.</p>
          <button class="ghost" id="signIn">Sign In</button>
        </div>
        <div class="overlay-panel overlay-right">
          <h1>Hello, Friend!</h1>
          <p>Share your personal interests with us on the next page</p>
          <button class="ghost" id="signUp">Interests</button>
        </div>
      </div>
    </div>
