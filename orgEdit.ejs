            <div class="col-9">
                <form class="" action = "/orgProfiles?_method=PUT" method = 'POST'>
                    <div class="mb-5">
                        <label  for= "orgName">Organization Name:</label>
                        <input type = "text" name = "orgName" id = "orgName" placeholder = "Profile name" value = "<%= foundUser.orgName%>">
                    </div>
                    <div class="mb-5">
                        <label for = "townLocation">Town Location:</label>
                        <input type = "text" name = "townLocation" id = "townlocation" placeholder = "Town Location" value = "<%= foundUser.townLocation%>">
                    </div>
                    <div class="mb-5">
                        <label for = "zipCode">Zip Code:</label>
                        <input type = "number" name = "zipCode" id = "zipCode" placeholder = "Zip Code" value = "<%= foundUser.zipCode%>">
                    </div>
                    <div class="mb-5">
                        <label for = "phoneNum">Phone number</label>
                        <input type = "number" name = "phoneNum" id = "phoneNum" placeholder = "Phone number" value = "<%= foundUser.phoneNum%>">
                    </div>
                    
                <% for(let category of categories2) { %>
                    <input type="checkbox" name= "<%= category %>" value = "<%= category %>" id= "<%= category %>"> </input>
                    <label for= "<%=category%>"><%= category %></label>

                <% } %> 
                    
                    <input type="submit">
                </form>
            </div>
        </div>
